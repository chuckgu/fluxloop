{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": null,
  "devCommand": "npm start",
  "installCommand": "npm install",
  "regions": ["icn1", "sfo1"],
  "github": {
    "silent": false,
    "autoAlias": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/docs/intro",
      "destination": "/docs/",
      "permanent": false
    }
  ]
}

